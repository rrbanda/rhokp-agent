# RHOKP ADK Agent Configuration
# Copy to .env and fill in values for your environment.

# Llama Stack endpoint (Responses API)
LLAMA_STACK_BASE_URL=

# LLM model identifier (from /v1/models on your Llama Stack instance)
# MODEL=gemini/models/gemini-2.5-pro

# MCP server URL for RHOKP search tool
MCP_SERVER_URL=http://localhost:8010/mcp

# RHOKP base URL (used by MCP server to reach Solr)
RHOKP_BASE_URL=http://localhost:8080
